{
  "include": [
    "src"
  ],
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2022",
    "lib": [
      "ES2022"
    ],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "isolatedModules": true,
    "moduleDetection": "force",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "extends": "expo/tsconfig.base"
}
